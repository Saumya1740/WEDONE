<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tradional_category">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logo"
        android:contentDescription="TODO" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/line"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="105dp"/>

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/searchicon"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="30dp"/>

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/carticon"
        android:layout_marginLeft="350dp"
        android:layout_marginTop="30dp"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="110dp"
        android:gravity="center_horizontal"
        android:text="Traditional Essentials"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="142dp"
        android:layout_height="200dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="160dp"
        app:srcCompat="@drawable/traditional1" />

    <TextView
        android:layout_width="220dp"
        android:layout_height="200dp"
        android:layout_marginLeft="170dp"
        android:layout_marginTop="160dp"
        android:background="@drawable/rectangle12"
        android:backgroundTint="#C8B59A"
        android:text="Now your shopping won't take too long..."
        android:textColor="#101957"
        android:textSize="20dp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/button3"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:text="For hindus"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="250dp"
        android:onClick="Openhindu"
        android:backgroundTint="#C8B59A"
        android:textColor="#101957"
        />
    <Button
        android:id="@+id/button4"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:text="For sikhs"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="250dp"
        android:onClick="Opensikhs"
        android:backgroundTint="#C8B59A"
        android:textColor="#101957"
        />
    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="142dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/traditional2"
        android:layout_marginTop="410dp"
        android:layout_marginLeft="250dp" />
    <TextView
        android:layout_width="220dp"
        android:layout_height="200dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="410dp"
        android:background="@drawable/rectangle12"
        android:backgroundTint="#C8B59A"
        android:text="Add a bit of tradition !"
        android:textColor="#101957"
        android:textSize="20dp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/button5"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:text="for muslims"
        android:layout_marginTop="495dp"
        android:onClick="Openmuslim"
        android:layout_marginLeft="58dp"
        android:backgroundTint="#C8B59A"
        android:textColor="#101957"
        />
    <Button
        android:id="@+id/button6"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:text="for christians"
        android:layout_marginTop="545dp"
        android:layout_marginLeft="58dp"
        android:onClick="Openchristians"
        android:backgroundTint="#C8B59A"
        android:textColor="#101957"
        />
    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="680dp"
        app:srcCompat="@drawable/line" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/categorybutton"
        android:layout_marginTop="690dp"
        android:layout_marginLeft="280dp"
        android:onClick="Opencategory"
        tools:ignore="OnClick" />


    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="72dp"
        android:layout_height="32dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="690dp"
        android:onClick="Opencategory"
        app:srcCompat="@drawable/backicon"
        tools:ignore="OnClick,VectorDrawableCompat" />



</androidx.coordinatorlayout.widget.CoordinatorLayout>