<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".contact_us">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="357dp"
        app:srcCompat="@drawable/finalbackground" />
    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="120dp"
        app:srcCompat="@drawable/trustedwritten"/>

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="210dp"
        app:srcCompat="@drawable/findherebox"/>
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="225dp"
        app:srcCompat="@drawable/findvendors"/>
    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="140dp"
        android:layout_marginTop="221dp"
        app:srcCompat="@drawable/searchicon2"/>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="300dp"
        app:srcCompat="@drawable/rectangleno" />





    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/wedone234"
        android:layout_marginTop="320dp"
        android:layout_marginLeft="130dp"
        />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="450dp"
        android:layout_marginLeft="70dp"
        android:layout_marginRight="10dp"
        android:textStyle="bold"
        android:textSize="15dp"
        android:textColor="#C8B59A"
        android:text="You are getting married and need help!!" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="490dp"
        android:layout_marginLeft="60dp"
        android:backgroundTint="#C8B59A"
        android:text="Mail Us" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="495dp"
        android:layout_marginLeft="170dp"
        android:textSize="20dp"
        android:textColor="#C8B59A"
        android:text="wedone@gmail.com" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="540dp"
        android:layout_marginLeft="60dp"
        android:backgroundTint="#C8B59A"
        android:text="Call Us"
        />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="545dp"
        android:layout_marginLeft="170dp"
        android:textSize="20dp"
        android:textColor="#C8B59A"
        android:text="9305244821" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Happy Wedding"
        android:layout_marginTop="620dp"
        android:layout_marginLeft="130dp"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="#C8B59A"
        />
</androidx.coordinatorlayout.widget.CoordinatorLayout>